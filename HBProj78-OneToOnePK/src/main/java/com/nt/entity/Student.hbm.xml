<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.nt.entity.Student" table="OTOPK_STUDENT">
        <id name="sid"  type="int">
               <generator class="increment"/>
         </id>
         <property name="sname"  length="15" type="string"/>
         <property name="saddrs"  length="15" type="string"/>
         <property name="dob" />
         <one-to-one name="libraryDetails" class="com.nt.entity.LibraryMembership"  cascade="all"  lazy="proxy"/>
      </class> 
 </hibernate-mapping>


